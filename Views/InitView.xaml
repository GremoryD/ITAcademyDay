<UserControl x:Class="ITAcademyDay.Views.InitView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ITAcademyDay.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <Style x:Key="ButtonsnewStyle"></Style>
    </UserControl.Resources>
    <Grid>
        <Button Command="{Binding NextWindowCommand}"  >
            <Button.Background>
                <ImageBrush ImageSource="{Binding imageBinding}"/>
            </Button.Background>
        </Button>
    </Grid>
</UserControl>
